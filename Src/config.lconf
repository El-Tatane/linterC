=extends
Src/config2.lconf

=rules
- unused-variable = on
- no-prototype = on
- indent = on
- array-Bracket-eol = on
- operators-spacing = on
- comma-spacing = on
- comments-header = on
- max-line-numbers = on
- no-trailing-spaces = on
- no-multi-declaration = off

- max-file-line-numbers = 10
- undeclared-variable = on
- unused-function = on
- undeclared-function = on
- variable-assignment-type = on
- function-parameters-type = on

=excludedFiles
AAAA.c
BBBB.c

=recursive
true