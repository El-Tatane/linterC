=extends
config/config2.lconf

=rules
- array-Bracket-eol = on
- operators-spacing = on
- comma-spacing = on
- indent = on
- comments-header = on
- max-line-numbers = on
- max-file-line-numbers = on
- no-trailing-spaces = on
- no-multi-declaration = on
- unused-variable = on
- undeclared-variable = on
- no-prototype = on
- unused-function = on
- undeclared-function = on
- variable-assignment-type = on
- function-parameters-type = on

=excludedFiles
test.c
=recursive
true